{
  "type":"origins:multiple",
  "number_of_jumps": {
    "type": "origins:resource",
    "min": 0,
    "max": 5,
    "hud_render": {
        "should_render": true,
		"sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
		"bar_index": 3
    }
  },
  "jump_reset": {
    "type":"origins:action_over_time",
    "interval": 1,
    "entity_action": {
        "type": "origins:change_resource",
        "resource": "*:*_number_of_jumps",
        "change": 1000
    },
    "condition":{
      "type":"origins:on_block"
    }
  },
  "activating_jump": {
    "type":"origins:active_self",
    "key": {
      "key":"key.jump"
    },
    "cooldown": 0,
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type":"origins:and",
      "conditions": [
        {
        "type":"origins:on_block",
        "inverted": true
        },
        {
          "type": "origins:resource",
          "resource": "*:*_number_of_jumps",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:lava",
          "comparison": "==",
          "compare_to": 0
        },
		{
		  "type": "origins:resource",
		  "resource": "squid:death-var",
		  "comparison": "==",
		  "compare_to": 1
		},
		{
          "type": "origins:equipped_item",
          "equipment_slot": "head",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }	
		},
        {
          "type": "origins:equipped_item",
          "equipment_slot": "chest",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "legs",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "feet",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
		},
		{
		  "type": "origins:passenger",
		  "comparison": "<",
		  "compare_to": 1
		},
		{
		  "type": "origins:health",
		  "comparison": ">=",
		  "compare_to": 8
		}
      ]
    },
    "entity_action": {
        "type": "origins:and",
        "actions": [
        {
          "type": "origins:add_velocity",
          "set": true,
          "y": 0.75
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:moving"
          },
          "if_action": {
            "type": "origins:add_velocity",
            "space":"local",
            "z": 0.35
          }
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_number_of_jumps",
          "change": -1
        }
      ]
    }
  },
  "slow_fall": {
    "type":"origins:active_self",
    "cooldown": 4,
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key":"key.jump",
      "continuous": true
    },
    "entity_action": {
          "type": "origins:apply_effect",
          "effect": {
            "effect":"minecraft:slow_falling",
            "show_icon": false,
            "show_particles": false,
            "duration": 5
          }
    },
    "condition": {
	  "type": "origins:and",
	  "conditions": [
	    {
		  "type": "origins:resource",
		  "resource": "*:*_number_of_jumps",
		  "comparison": "==",
		  "compare_to": 0
		},
		{
		  "type": "origins:resource",
		  "resource": "squid:death-var",
		  "comparison": "==",
		  "compare_to": 1
		},
		{
          "type": "origins:equipped_item",
          "equipment_slot": "head",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }	
		},
        {
          "type": "origins:equipped_item",
          "equipment_slot": "chest",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "legs",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "feet",
          "item_condition": {
            "type": "origins:armor_value",
            "comparison": "==",
            "compare_to": 0
          }
		},
		{
		  "type": "origins:health",
		  "comparison": ">=",
		  "compare_to": 8
		}
      ]
	}
  },
  "hidden": true
}
